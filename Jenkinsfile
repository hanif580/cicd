pipeline {
    agent any
    	stages {

        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Test') {
            steps {
			echo 'This is test file'
            }
        }

        stage('Push') {
            steps {
			echo 'Push the file'
            }
        }

        stage('Deploy') {
            steps {
			echo 'Deploy the file'
            }
        }
    }
} 

